datasource postgres {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

datasource mongodb {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Quest {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  createdAt   DateTime @default(now())
}

model Order {
  id        Int      @id @default(autoincrement())
  questId   Int
  userId    Int
  status    String
  createdAt DateTime @default(now())
}
